    private void imageNewButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_imageNewButtonActionPerformed
        jFileChooser1 = new javax.swing.JFileChooser();
        jFileChooser1.setDialogTitle("选择图片");
        String fileName = null;
        jFileChooser1.setFileFilter(new FileNameExtensionFilter("图片文件(*.bmp, *.gif, *.jpg, *.jpeg, *.png)", "bmp", "gif", "jpg", "jpeg", "png"));
        if (jFileChooser1.showOpenDialog(null) == jFileChooser1.APPROVE_OPTION) {
            fileName = jFileChooser1.getSelectedFile().getAbsolutePath();
            if (fileName == null) {
                return;
            }
        } else {
            return;
        }
        System.out.println(fileName);
        this.imageArrayList.add(fileName);
        // TODO refresh imageList
        //this is a listener mode
        imageArrayListUpdate();
// TODO add your handling code here:
    }//GEN-LAST:event_imageNewButtonActionPerformed

