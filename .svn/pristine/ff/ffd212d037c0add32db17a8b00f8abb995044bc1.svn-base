    private String[] getImageList() {
        if (imageArrayList != null) {
            String[] result = new String[imageArrayList.size()];
            for (int i = 0; i < imageArrayList.size(); i++) {
                result[i] = imageArrayList.get(i);
            }
            return result;
        }
        return null;
    }

