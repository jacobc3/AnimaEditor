    private void frameUpButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_frameUpButtonActionPerformed

        System.out.println("@Frame Up:You have selected : " + frameList.getSelectedIndex());
        if (frameList.isSelectionEmpty() || frameList.getSelectedIndex() == 0) {
            return;
        }
        int index = frameList.getSelectedIndex();
        FrameInterface temp = frameArrayList.get(index);
        frameArrayList.set(index, frameArrayList.get(index - 1));
        frameArrayList.set(index - 1, temp);
        frameList.setSelectedIndex(index - 1);
        frameList.repaint();
    }//GEN-LAST:event_frameUpButtonActionPerformed

    private void frameDownButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_frameDownButtonActionPerformed
        System.out.println("@Frame Up:You have selected : " + frameList.getSelectedIndex() + "/" + frameArrayList.size());

        if (frameList.isSelectionEmpty() || frameList.getSelectedIndex() == frameArrayList.size() - 1) {
            return;
        }
        int index = frameList.getSelectedIndex();
        FrameInterface temp = frameArrayList.get(index);
        frameArrayList.set(index, frameArrayList.get(index + 1));
        frameArrayList.set(index + 1, temp);
        frameList.setSelectedIndex(index + 1);
        frameList.repaint();
    }//GEN-LAST:event_frameDownButtonActionPerformed

